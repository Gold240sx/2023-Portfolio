        "husky": "^8.0.3",
        "lint-staged": "^13.1.0",
        "postcss": "^8.4.21",
        "prettier": "^2.8.3",
        "prettier-plugin-tailwindcss": "^0.2.4",
        "tailwindcss": "^3.2.4",
        "vite": "^4.1.0",
        "vitest": "^0.28.4"
    }
